{
  "name": "vite-react-typescript-starter",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "dev:full": "concurrently \"npm run server:dev\" \"npm run dev\"",
    "server:dev": "cd server && npm install && npm run dev",
    "server:start": "cd server && npm install && npm start",
    "prebuild": "node build.cjs --prebuild",
    "build": "tsc && vite build",
    "postbuild": "node build.cjs --postbuild",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "preview": "vite preview",
    "convert-json": "node scripts/convert-json-to-csv.js",
    "convert-catalogs": "node scripts/convert-catalogs-to-csv.js",
    "convert-enhanced": "node scripts/enhanced-catalog-converter.js"
  },
  "dependencies": {
    "@headlessui/react": "^1.7.17",
    "@heroicons/react": "^2.0.18",
    "@supabase/supabase-js": "^2.33.2",
    "@types/papaparse": "^5.3.16",
    "axios": "^1.11.0",
    "body-parser": "^2.2.0",
    "cheerio": "^1.1.2",
    "cors": "^2.8.5",
    "dotenv": "^17.2.2",
    "emailjs-com": "^3.2.0",
    "express": "^5.1.0",
    "firebase": "^11.10.0",
    "framer-motion": "^10.16.4",
    "lucide-react": "^0.525.0",
    "node-cache": "^5.1.2",
    "papaparse": "^5.5.3",
    "puppeteer": "^24.19.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-hot-toast": "^2.4.1",
    "react-router-dom": "^6.15.0",
    "sharp": "^0.34.3"
  },
  "devDependencies": {
    "@eslint/js": "^9.9.1",
    "@types/react": "^18.3.5",
    "@types/react-dom": "^18.3.0",
    "@vitejs/plugin-react": "^4.3.1",
    "autoprefixer": "^10.4.18",
    "concurrently": "^9.2.1",
    "eslint": "^9.9.1",
    "eslint-plugin-react-hooks": "^5.1.0-rc.0",
    "eslint-plugin-react-refresh": "^0.4.11",
    "globals": "^15.9.0",
    "postcss": "^8.4.35",
    "tailwindcss": "^3.4.1",
    "ts-node": "^10.9.2",
    "typescript": "^5.5.3",
    "typescript-eslint": "^8.3.0",
    "vite": "^5.4.2"
  }
}
